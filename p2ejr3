//codificar un programa que solicite la carga de un valor positivo y nos muestre desde 1 hasta
//el valor ingresado de uno en uno. Ejemplo: Si ingresamos 30 se debe mostrar en pantalla losnúmeros del 1 al 30.

//tuve que recurrir a un compañero para terminar de entender este problema

int contador = 0;

Console.WriteLine("ingresar un número: ");
int num = int.Parse(Console.ReadLine());

Console.WriteLine("Los números del 1 al " + num + " son: ");

while (contador <= num)
{
    Console.WriteLine(contador);
    contador++;
}
